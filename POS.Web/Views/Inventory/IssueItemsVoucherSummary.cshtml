
@{
    ViewBag.Title = "Issue Items Voucher Summary";
}


<div id="VoucherSummary"></div>

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script>
    $(document).ready(function () {
        GetIssueItemsVoucherSummary();
    });

    function GetIssueItemsVoucherSummary() {
        startLoader();
        $.ajax(
            {
                url: "/Inventory/GetIssueItemsVoucherSummary/",
                type: "Get"
            }
        ).done(function (result) {
            $("#VoucherSummary").html(result);
            stopLoader();
        });
    }
</script>

